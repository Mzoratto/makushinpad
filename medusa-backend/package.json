{
  "name": "shin-shop-backend-clean",
  "version": "2.0.0",
  "description": "Medusa.js backend for The Shin Shop - Clean deployment without mollie plugin",
  "author": "The Shin Shop",
  "license": "MIT",
  "keywords": [
    "medusa-backend",
    "ecommerce",
    "shin-pads",
    "custom-products"
  ],
  "scripts": {
    "clean": "./node_modules/.bin/rimraf dist",
    "build": "NODE_ENV=production npm run build:prod",
    "build:prod": "npm run clean && tsc -p tsconfig.json",
    "build:server": "cross-env npm run clean && tsc -p tsconfig.json",
    "build:admin": "cross-env medusa-admin build --skip-build",
    "build:full": "cross-env npm run clean && npm run build:server && npm run build:admin",
    "watch": "cross-env tsc --watch",
    "test": "cross-env jest",
    "seed": "cross-env medusa seed -f ./data/seed.json",
    "start": "NODE_ENV=production medusa start",
    "start:custom": "cross-env npm run build && node --preserve-symlinks --trace-warnings index.js",
    "dev": "npm run build:server && medusa develop",
    "migrate": "cross-env medusa migrations run",
    "test:mollie": "node test-mollie-integration.js",
    "products:manage": "node manage-products.js",
    "products:seed-enhanced": "node seed-enhanced-products.js",
    "products:seed-enhanced-run": "npm run products:seed-enhanced && npm run seed -- -f ./data/seed-enhanced.json",
    "verify": "node verify-install.js"
  },
  "dependencies": {
    "@medusajs/admin": "^7.1.14",
    "@medusajs/cache-inmemory": "^1.8.10",
    "@medusajs/cache-redis": "^1.9.0",
    "@medusajs/event-bus-local": "^1.9.7",
    "@medusajs/event-bus-redis": "^1.8.13",
    "@medusajs/file-local": "^1.0.3",
    "@medusajs/medusa": "^1.20.6",
    "@medusajs/medusa-cli": "^1.3.22",
    "babel-preset-medusa-package": "^1.1.19",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "16.3.1",
    "express": "^4.17.2",
    "medusa-fulfillment-manual": "^1.1.40",
    "medusa-interfaces": "^1.3.7",
    "medusa-payment-stripe": "^6.0.6",
    "@mollie/api-client": "^3.7.0",
    "node-fetch": "^2.6.7",
    "prism": "^4.1.2",
    "typeorm": "^0.3.16",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.21.4",
    "@medusajs/medusa-cli": "^1.3.22",
    "@stdlib/number-float64-base-normalize": "0.0.8",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/mime": "1.3.5",
    "@types/node": "^17.0.8",
    "babel-preset-medusa-package": "^1.1.19",
    "eslint": "^6.8.0",
    "jest": "^27.3.1",
    "mongoose": "^5.13.14",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
