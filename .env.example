# Shin Shop Environment Variables
# Copy this file to .env.production for production deployment

# Snipcart Configuration
GATSBY_SNIPCART_API_KEY=your_production_snipcart_public_api_key_here

# Site Configuration
GATSBY_SITE_URL=https://your-domain.com

# Analytics (Optional)
GATSBY_GA_TRACKING_ID=your_google_analytics_tracking_id

# Stripe (if using direct Stripe integration)
GATSBY_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key

# Development vs Production
NODE_ENV=production
